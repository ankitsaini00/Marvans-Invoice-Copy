                                                <select class="custom-select" name="payment_type" required>
                                                  <option selected value="Cash">Cash</option>
                                                  <option value="Card">50-50</option>
                                                  <option value="Online">Online</option>
                                                </select>
                                                
                                            </div>
                                        </div>

                                        <div class="col-md-12 mb-2">
                                            <label for="exampleFormControlInput2">Total: </label>
                                            <input type="number" min="0" name="total" value="<%=order.total%>" required class="form-control"  placeholder="Total" >
                                        </div>  
                                        <div class="col-md-12 mb-2">
                                            <label for="exampleFormControlInput2">Discount(%): </label>
                                            <input step="0.01" type="number" min="0" name="discount" value="<%=order.discount%>" required class="form-control"  placeholder="Total" >
