                    <div class="">
                        <form target="_blank" action="/new/create-pdf/<%=order._id%>" method="POST" id="save-details">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-row">
                                        <div class="col-md-12 mb-2">
                                            <label for="exampleFormControlInput2">Customer Name : </label>
                                            <input type="text" name="username" required value="<%=order.customer.name%>" class="form-control" id="exampleFormControlInput2" placeholder="Ankit Saini" >
                                        </div>   
                                        <div class="col-md-12 mb-2">
                                            <label for="exampleFormControlInput2">City: </label>
                                            <input type="text" name="city" required value="<%=order.customer.city%>" class="form-control" id="exampleFormControlInput2" placeholder="Vadodra" >
                                        </div>   
                                        <div class="col-md-12 mb-2">
                                            <div id="select_menu" class="form-group mb-4">
